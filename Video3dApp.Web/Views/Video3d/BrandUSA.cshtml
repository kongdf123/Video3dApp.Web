	<script type="text/javascript" src="/Scripts/aframe-master.min.js"></script>
	<script type="text/javascript">
		$(function () {
			$('#scrollpage').fullpage({
				navigation: true
			});
			$('.tbn-model').click(function () {
				$('.tk-model').show();
			})
			$('.model-close').click(function () {
				$('.tk-model').hide();
			})
			$("#btnPlay").click(function () {
				location.href = "/Video3d/Detail";
				return false;
			});
			document.querySelector("#galleryWrapper").innerHTML = "<a-scene><a-assets><img src='http://picture.thomascook.com.cn/active/american-brand/F2.png' crossorigin='anonymous' id='vrImage'/></a-assets><a-sky src='#vrImage' crossorigin='anonymous' ></a-sky>/a-scene>";
		});

